#include<stdio.h>
struct date
{
	int year;
	int month;
	int day;
};
int days(struct date b);
int main() {
	struct date a;
	scanf("%d %d %d",&a.year,&a.month,&a.day);
	printf("%d", days(a));
	return 0;
}
int days(struct date c) {
	int a[12] = { 31,28,31,30,31,30,31,31,30,31,30,31 };//平年
	int b[12] = { 31,29,31,30,31,30,31,31,30,31,30,31 };//闰年
	int i,sum=0;
	if (c.year%400==0 || c.year%4==0 && c.year%100!=0)
	{
		for (i = 0; i < c.month-1; i++) {
			sum += b[i];
		}
		sum += c.day;
	}
	else
	{
		for (i = 0; i < c.month-1; i++) {
			sum += a[i];
		}
		sum += c.day;
	}
	return sum;
}
